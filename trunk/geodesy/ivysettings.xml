<ivysettings>
	<!-- Create a ivy.properties file in your home dir and put a line in with your mapsrv user and passwd -->
	<properties file="${user.home}/ivy.properties"/>
	<property name="repository.dir" value="${user.home}/.ivy2"/>
	<settings defaultResolver="chained"/>
	<credentials host="maven-proxy" realm="Artifactory Realm" username="${mapsrv-user}"
		passwd="${mapsrv-passwd}"/>
	<namespaces>
		<namespace name="m2">
			<rule>
				<fromsystem>
					<src org="servletapi"/>
					<dest org="javax.servlet"/>
				</fromsystem>
				<tosystem>
					<src org="javax.servlet"/>
					<dest org="servletapi"/>
				</tosystem>
			</rule>
			<rule>
				<fromsystem>
					<src org="com.sun.xml"/>
					<dest org="javax.xml"/>
				</fromsystem>
				<tosystem>
					<src org="javax.xml"/>
					<dest org="com.sun.xml"/>
				</tosystem>
			</rule>
		</namespace>
	</namespaces>
	<resolvers>
		<chain name="chained">
			<filesystem name="libraries">
				<ivy pattern="${repository.dir}/[organisation]/ivy-[revision].xml"/>
				<artifact
					pattern="${repository.dir}/[organisation](-[module])/[artifact]-[revision](-[type]).[ext]"
				/>
			</filesystem>
			<ibiblio name="maven-proxy" namespace="m2" m2compatible="true"
				root="http://maven-proxy/artifactory/repo/"
				pattern="[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
		</chain>
		<url name="maven-proxy-local" m2compatible="true">
			<artifact
				pattern="http://maven-proxy/artifactory/libs-releases-local/[organisation]/[module]/[revision]/[artifact]-[revision](-[type]).[ext]"
			/>
		</url>
	</resolvers>
</ivysettings>
