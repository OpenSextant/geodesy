<ivysettings>
	<property name="repository.dir" value="${user.home}/.ivy2" />
	<settings defaultResolver="chained" />
	<namespaces>
		<namespace name="m2">
			<rule>
				<fromsystem>
					<src org="servletapi" />
					<dest org="javax.servlet" />
				</fromsystem>
				<tosystem>
					<src org="javax.servlet" />
					<dest org="servletapi" />
				</tosystem>
			</rule>
			<rule>
				<fromsystem>
					<src org="com.sun.xml" />
					<dest org="javax.xml" />
				</fromsystem>
				<tosystem>
					<src org="javax.xml" />
					<dest org="com.sun.xml" />
				</tosystem>
			</rule>
		</namespace>
	</namespaces>
	<resolvers>
		<chain name="chained">
			<filesystem name="libraries">
				<ivy pattern="${repository.dir}/[organisation]/ivy-[revision].xml" />
				<artifact
					pattern="${repository.dir}/[organisation](-[module])/[artifact](-[type])-[revision].[ext]" />
			</filesystem>

			<ibiblio name="mapsrv" namespace="m2" checkconsistency="false"
				root="http://mapsrv.mitre.org/maven-proxy" m2compatible="true" />
		</chain>
		<ssh name="ssh" host="mapsrv">
			<artifact pattern="/data/m2/repo-local/org/mitre/itf/[module]/[revision]/[artifact]-[revision].[ext]" />
		</ssh>
	</resolvers>
</ivysettings>
